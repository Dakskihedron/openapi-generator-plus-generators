package {{apiPackage}};

/**
 * The server returned a status code that is not documented in the API.
 */
public class UnexpectedResponseException extends java.lang.RuntimeException {

	private static final long serialVersionUID = 1L;

	private javax.ws.rs.core.Response response;
	private javax.ws.rs.WebApplicationException webApplicationException;

	public UnexpectedResponseException(javax.ws.rs.core.Response response, javax.ws.rs.WebApplicationException cause) {
		super("Unexpected response status " + response.getStatus(), cause);
		this.response = response;
		this.webApplicationException = cause;
	}

	public javax.ws.rs.core.Response getResponse() {
		return response;
	}

	public javax.ws.rs.WebApplicationException getWebApplicationException() {
		return webApplicationException;
	}

}

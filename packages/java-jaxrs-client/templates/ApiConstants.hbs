package {{apiPackage}};

public interface ApiConstants {

	{{#if server.description}}
	/**
	 * {{{md server.description}}}
	 */
	{{/if}}
	String DEFAULT_SERVER = {{{stringLiteral server.url}}};

	{{#each servers}}
	{{#if description}}
	/**
	 * {{{md description}}}
	 */
	{{/if}}
	{{#if vendorExtensions.x-name}}
	String {{constantName (concat vendorExtensions.x-name 'Server')}} = {{{stringLiteral url}}};
	{{else}}
	String {{constantName (concat 'Server' '_' @index)}} = {{{stringLiteral url}}};
	{{/if}}

	{{/each}}
	{{>hooks/apiConstantsBody}}
}

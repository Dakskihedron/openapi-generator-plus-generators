{
	"name": "{{name}}",
	{{#if private}}
	"private": true,
	{{/if}}
	"version": "{{version}}",
	"description": "API client for {{name}}",
	"author": "{{{generatorClass}}}",
	"keywords": [
		"fetch",
		"typescript",
		"swagger",
		"openapi",
		"{{name}}"
	],
	"license": "UNLICENSED",
	"main": "./dist/index.js",
	"typings": "./dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"prepare": "npm run build"
	},
	"dependencies": {
{{#join '__dependencies' ',
		'}}
{{>hooks/packageDependencies}}
{{#ifeq dateApproach 'blind-date'}}
"blind-date": "^3.3.0"
{{/ifeq}}
{{/join}}
		{{{__dependencies}}}
	},
	"devDependencies": {
		"@types/node": "^20.14.8",
		"typescript": "^5.9.2"
	{{#if repository}}
	},
	"publishConfig": {
		"registry": "{{{repository}}}"
	{{/if}}
	}
}

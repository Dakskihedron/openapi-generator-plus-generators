{{!--
	Renders the definition-section of the HTML-page.
	@param {Definition[]} definitions a list of JSON-subschemas.
	@api public
--}}

<article id="definitions" class="content">
	<h2>Definitions</h2>

	{{#eachSorted schemas}}
		{{#unless anonymous}}
		{{>api/model model=. anchor="/definitions" }}
		{{/unless}}
	{{/eachSorted}}
</article>

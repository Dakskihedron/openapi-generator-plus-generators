{{!Renders model discriminator}}
<div class="params -discriminator">
    <h5>Discriminator</h5>
	{{#with discriminator}}
	<div class="params -properties">
		<table>
			<thead>
				<tr>
					<th>Name</th>
					<th>Type</th>
				</tr>
			</thead>
			<tr data-property-name="{{name}}">
				<td class="name">
					{{name}}
					{{#if required}}<span class="type">Required</span>{{/if}}
				</td>
				<td class="type">
					{{>model/datatype anchor=../anchor}}
				</td>
				<td class="desc">
					{{{md description}}}
				</td>
			</tr>
		</table>
	</div>
	{{/with}}
</div>
<div class="params -discriminator">
	<h5>Discriminator values</h5>
	<div class="params -properties">
		<table>
			<thead>
				<tr>
					<th>Value</th>
					<th>Reference</th>
				</tr>
			</thead>
			<tbody>
				{{#each discriminator.references}}
				<tr>
					<td>{{name}}</td>
					<td><a class="model-ref" href="#{{../anchor}}/{{htmlId model.nativeType}}">{{model.nativeType}}</a></td>
				</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>

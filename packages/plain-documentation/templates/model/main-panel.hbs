{{!
    Renders a model inside a bootstrap-panel.
    @public
    @readonly
}}

                <section class="summary" data-class-title="{{name}}">
                    {{#if anchor}}<a name="{{anchor}}/{{htmlId nativeType}}"></a>{{/if}}
                    {{#if name}}
                        <h4>
                            {{nativeType}}
                            {{#if (isObject)}}
                            {{#if isInterface}}<span class="type">INTERFACE</span>{{/if}}
                            {{/if}}
                            {{#each allOf}}{{#if $ref}}<span class="type">&lt; {{>model/reference $ref}}</span>{{/if}}{{/each}}
                        </h4>
                    {{/if}}

                    {{>model/body anchor=anchor}}

                    {{#each schemas}}
                        {{>model/main-panel anchor=../anchor}}
                    {{/each}}
                </section>

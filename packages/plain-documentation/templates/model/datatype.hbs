<span class="model-property-type">
    {{#if anchor}}
        {{#if (isObject)}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId nativeType}}">{{nativeType}}</a>
        {{else if (isMap)}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId componentSchema.nativeType}}">{{nativeType}}</a>
        {{else if (isArray)}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId componentSchema.nativeType}}">{{nativeType}}</a>
        {{else if (isEnum)}}
            <a class="model-ref" href="#{{anchor}}/{{htmlId nativeType}}">{{nativeType}}</a>
        {{else}}
            {{nativeType}}
            {{#if nativeType.serializedType}}
            ({{nativeType.serializedType}})
            {{/if}}
        {{/if}}
    {{else}}
        {{nativeType}}
    {{/if}}
</span>
{{#if defaultValue}}
    <span class="model-property-default-value">{{defaultValue.value}}</span>
{{/if}}

{
  "name": "openapi-generator-plus-generators",
  "private": true,
  "scripts": {
    "build": "pnpm build --recursive",
    "clean": "pnpm clean --recursive",
    "clean:node_modules": "pnpm --recursive exec rm -rf node_modules",
    "link": "pnpm link --global @openapi-generator-plus/core @openapi-generator-plus/types @openapi-generator-plus/indexed-type @openapi-generator-plus/testing && pnpm --recursive --workspace-concurrency=1 exec pnpm link --global",
    "release": "pnpm clean && pnpm build && pnpm test && pnpx changeset publish",
    "test": "pnpm test --recursive",
    "watch": "pnpm watch --parallel"
  },
  "devDependencies": {
    "@changesets/cli": "^2.17.0",
    "@types/jest": "^27.0.1",
    "@types/less": "^3.0.3",
    "@types/lodash": "^4.14.172",
    "@types/marked": "^2.0.4",
    "@types/node": "^16.6.1",
    "@types/pluralize": "0.0.29",
    "@types/rimraf": "^3.0.1",
    "@types/url-parse": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "glob-promise": "^4.2.0",
    "handlebars": "^4.7.7",
    "jest": "^27.0.6",
    "maven": "github:karlvr/node-maven",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@openapi-generator-plus/core": "^0.31.0",
    "@openapi-generator-plus/indexed-type": "^0.1.6",
    "@openapi-generator-plus/testing": "^0.31.0",
    "@openapi-generator-plus/types": "^0.31.0"
  }
}

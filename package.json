{
  "name": "openapi-generator-plus-generators",
  "private": true,
  "scripts": {
    "build": "pnpm --recursive build",
    "check-updates": "pnpx npm-check-updates -u && pnpm --recursive exec pnpx npm-check-updates -u",
    "clean": "pnpm --recursive clean",
    "clean:node": "pnpm --recursive exec rm -rf node_modules && rm -rf node_modules",
    "link": "pnpm link -D --global @openapi-generator-plus/testing && pnpm --recursive --workspace-concurrency=1 exec pnpm run link",
    "release": "pnpm clean && pnpm build && pnpm test && pnpx changeset publish && pnpm install",
    "release:version": "pnpm changeset version && pnpm install",
    "test": "pnpm --recursive --workspace-concurrency 1 test",
    "watch": "pnpm --parallel watch"
  },
  "devDependencies": {
    "@changesets/cli": "^2.21.1",
    "@types/jest": "^27.4.1",
    "@types/less": "^3.0.3",
    "@types/lodash": "^4.14.179",
    "@types/marked": "^4.0.2",
    "@types/node": "^17.0.21",
    "@types/pluralize": "0.0.29",
    "@types/rimraf": "^3.0.2",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.11.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "maven": "github:karlvr/node-maven",
    "openapi-types": "^11.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@openapi-generator-plus/testing": "^1.6.0"
  }
}
